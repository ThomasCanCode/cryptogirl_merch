generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user_wallet {
  id     Int     @unique(map: "id") @default(autoincrement())
  wallet String  @unique(map: "wallet") @db.VarChar(50)
  date   BigInt?
  points Float?  @default(0) @db.Float
}

model orders {
  id         Int    @id @default(autoincrement())
  user_info  String @db.VarChar(500)
  order_info String @db.VarChar(9999)
}
